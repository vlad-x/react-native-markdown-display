    "react-native",
    "native",
    "markdown",
    "commonmark",
    "markdown-it"
  ],
  "author": "Mient-jan Stelling and Tom Pickard + others from the community",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iamacup/react-native-markdown-display/issues"
  },
  "homepage": "https://github.com/iamacup/react-native-markdown-display/",
  "dependencies": {
    "css-to-react-native": "^3.0.0",
    "markdown-it": "^10.0.0",
    "prop-types": "^15.7.2",
    "react-native-fit-image": "^1.5.5"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-native": "^0.65.1 || ^0.66.4 || ^0.68.1"
  },
  "devDependencies": {
    "@types/markdown-it": "^12.2.1",
    "@types/react-native": "^0.64.13",
    "@babel/core": "^7.15.0",
    "@babel/runtime": "^7.15.3",
    "@react-native-community/eslint-config": "^3.0.0",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "json-schema": "^0.3.0",
    "pre-commit": "^1.2.2",
    "typescript": "^4.4.2"
  }
